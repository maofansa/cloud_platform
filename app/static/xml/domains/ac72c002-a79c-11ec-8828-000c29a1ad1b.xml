<domain type="kvm">
  <name>Test4</name>
  <uuid>ac72c002-a79c-11ec-8828-000c29a1ad1b</uuid>
  <title />
  <description />
  <memory unit="KiB">1048576</memory>
  <currentMemory unit="KiB">1048576</currentMemory>
  <vcpu placement="static">1</vcpu>
  <resource>
    <partition>/machine</partition>
  </resource>
  <os>
    <type arch="x86_64" machine="pc-i440fx-rhel7.0.0">hvm</type>
    <boot dev="cdrom" />
    <boot dev="hd" />
  </os>
  <features>
    <acpi />
    <apic />
  </features>
  <cpu check="full" match="exact" mode="custom">
    <model fallback="forbid">Broadwell-IBRS</model>
    <feature name="md-clear" policy="require" />
    <feature name="ssbd" policy="require" />
    <feature name="hle" policy="disable" />
    <feature name="rtm" policy="disable" />
    <feature name="spec-ctrl" policy="require" />
    <feature name="hypervisor" policy="require" />
    <feature name="xsaveopt" policy="require" />
  </cpu>
  <clock offset="utc">
    <timer name="rtc" tickpolicy="catchup" />
    <timer name="pit" tickpolicy="delay" />
    <timer name="hpet" present="no" />
  </clock>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>restart</on_reboot>
  <on_crash>destroy</on_crash>
  <pm>
    <suspend-to-mem enabled="no" />
    <suspend-to-disk enabled="no" />
  </pm>
  <devices>
    <emulator>/usr/libexec/qemu-kvm</emulator>
    <disk device="disk" type="file">
      <driver name="qemu" type="qcow2" />
      <source file="/root/libvirt/img/test4.img" />
      <backingStore />
      <target bus="ide" dev="hda" />
      <alias name="ide0-0-0" />
      <address bus="0" controller="0" target="0" type="drive" unit="0" />
    </disk>
    <disk device="cdrom" type="file">
      <driver name="qemu" type="raw" />
      <source file="/root/libvirt/iso/CentOS-7-x86_64-DVD-2009.iso" />
      <backingStore />
      <target bus="ide" dev="hdb" />
      <readonly />
      <alias name="ide0-0-1" />
      <address bus="0" controller="0" target="0" type="drive" unit="1" />
    </disk>
    <controller index="0" model="ich9-ehci1" type="usb">
      <alias name="usb" />
      <address bus="0x00" domain="0x0000" function="0x7" slot="0x04" type="pci" />
    </controller>
    <controller index="0" model="ich9-uhci1" type="usb">
      <alias name="usb" />
      <master startport="0" />
      <address bus="0x00" domain="0x0000" function="0x0" multifunction="on" slot="0x04" type="pci" />
    </controller>
    <controller index="0" model="ich9-uhci2" type="usb">
      <alias name="usb" />
      <master startport="2" />
      <address bus="0x00" domain="0x0000" function="0x1" slot="0x04" type="pci" />
    </controller>
    <controller index="0" model="ich9-uhci3" type="usb">
      <alias name="usb" />
      <master startport="4" />
      <address bus="0x00" domain="0x0000" function="0x2" slot="0x04" type="pci" />
    </controller>
    <controller index="0" model="pci-root" type="pci">
      <alias name="pci.0" />
    </controller>
    <controller index="0" type="ide">
      <alias name="ide" />
      <address bus="0x00" domain="0x0000" function="0x1" slot="0x01" type="pci" />
    </controller>
    <interface type="network">
      <source bridge="virbr0" network="default" />
      <target dev="vnet2" />
      <model type="virtio" />
      <alias name="net0" />
      <address bus="0x00" domain="0x0000" function="0x0" slot="0x03" type="pci" />
    </interface>
    <serial type="pty">
      <source path="/dev/pts/2" />
      <target port="0" type="isa-serial">
        <model name="isa-serial" />
      </target>
      <alias name="serial0" />
    </serial>
    <console tty="/dev/pts/2" type="pty">
      <source path="/dev/pts/2" />
      <target port="0" type="serial" />
      <alias name="serial0" />
    </console>
    <input bus="ps2" type="mouse">
      <alias name="input0" />
    </input>
    <input bus="ps2" type="keyboard">
      <alias name="input1" />
    </input>
    <graphics autoport="yes" listen="192.168.137.9" port="5902" type="vnc">
      <listen address="192.168.137.9" type="address" />
    </graphics>
    <video>
      <model heads="1" primary="yes" type="cirrus" vram="16384" />
      <alias name="video0" />
      <address bus="0x00" domain="0x0000" function="0x0" slot="0x02" type="pci" />
    </video>
    <memballoon model="virtio">
      <alias name="balloon0" />
      <address bus="0x00" domain="0x0000" function="0x0" slot="0x05" type="pci" />
    </memballoon>
  </devices>
  <seclabel model="dac" relabel="yes" type="dynamic">
    <label>+0:+0</label>
    <imagelabel>+0:+0</imagelabel>
  </seclabel>
</domain>